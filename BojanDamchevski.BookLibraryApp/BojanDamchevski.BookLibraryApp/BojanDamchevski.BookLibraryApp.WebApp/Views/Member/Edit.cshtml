@model AddMemberViewModel

<h3>Edit member</h3>
<div class="row">
    <div class="col-md-6">
        <form asp-action="Edit" method="post">
            <input type="hidden" asp-for="NewId" />
            <div class="form-group">
                <label asp-for="NewFirstName" class="custom-control-label"></label>
                <input type="text" asp-for="NewFirstName" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="NewLastName" class="custom-control-label"></label>
                <input type="text" asp-for="NewLastName" class="form-control" />
            </div>
            <button type="submit" class="btn btn-success">Edit</button>
        </form>
    </div>
</div>